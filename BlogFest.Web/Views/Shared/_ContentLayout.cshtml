
@{
	Layout = "_Layout.cshtml";
}
@section Styles{
    @await RenderSectionAsync("Styles", false)
}

<div class="container">
    <div class="row">
        <div class="col-lg-9">
            @RenderBody()
        </div>

        <div class="col-md-3">

           @await Component.InvokeAsync("SideBar")
        </div>
    </div>
</div>

@section Scripts{
    @await RenderSectionAsync("Scripts", false)
}