@using BlogFest.Web.ViewModels;
@model List<ErrorItemViewModel>
@if (Model.Count == 0) return;

<ul>
	@foreach (var item in Model)
	{

		<li>
			<div class="alert alert-danger" role="alert">
				@item.Key:
				@foreach (var message in item.Message)
				{
					@message
				}
			</div>
		</li>

	}

</ul>